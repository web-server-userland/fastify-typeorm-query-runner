{
  "name": "@web-server-userland/fastify-typeorm-query-runner",
  "version": "1.0.0",
  "description": "Fastify typeorm QueryRunner",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "lint": "pnpm lint:standard && pnpm lint:typescript",
    "lint:fix": "standard --fix",
    "lint:standard": "standard",
    "lint:typescript": "standard --parser @typescript-eslint/parser --plugin @typescript-eslint/eslint-plugin test/*.ts",
    "typescript": "tsd",
    "unit": "ava test/*.test.js",
    "test": "pnpm lint && nyc --reporter=html pnpm unit && pnpm typescript",
    "test:ci": "pnpm lint && nyc --reporter=lcov pnpm unit && pnpm typescript"
  },
  "keywords": [
    "fastify",
    "typeorm",
    "transaction",
    "queryrunner",
    "fastify-plugin"
  ],
  "files": [
    "index.js",
    "index.d.ts"
  ],
  "author": "Black-Hole <158blackhole@gmail.com>",
  "homepage": "https://github.com/web-server-userland/fastify-typeorm-query-runner#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/web-server-userland/fastify-typeorm-query-runner.git"
  },
  "bugs": {
    "url": "https://github.com/web-server-userland/fastify-typeorm-query-runner/issues"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "ava": "^4.3.0",
    "fastify": "^4.2.0",
    "nyc": "^15.1.0",
    "standard": "^17.0.0",
    "tsd": "^0.21.0",
    "typeorm": "^0.3.7",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "fastify-plugin": "^3.0.1"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "eslint"
      ]
    }
  },
  "tsd": {
    "directory": "test"
  },
  "publishConfig": {
    "access": "public"
  }
}
